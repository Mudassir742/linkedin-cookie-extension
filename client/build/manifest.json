{
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "version": "0.1",
  "description": "LinkedIn Mini Profile",
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": ["cookies"],
  "host_permissions": ["https://www.google.com/", "https://www.linkedin.com/feed/"],
  "short_name": "React Extension",
  "action": {
    "default_popup": "popup.html",
    "default_title": "LinkedIn MiniProfile",
    "default_icon": {
      "16": "logo192.png",
      "32": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    }
  },
  "icons": {
    "16": "logo192.png",
    "32": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
